{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "eslint:recommended",
        "plugin:react/recommended",
        "prettier",
        "airbnb"
    ],
    "overrides": [],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module",
        "requireConfigFile": false
    },
    "plugins": [
        "react"
    ],
    "rules": {
        "no-console": "off",
        "no-param-reassign": [
            2,
            {
                "props": false
            }
        ],
        "react/prop-types": 0,
        "jsx-a11y/no-autofocus": [
            2,
            {
                "ignoreNonDOM": true
            }
        ],
        "import/no-extraneous-dependencies": [
            "error",
            {
                "devDepencies": false
            }
        ]
    }
}